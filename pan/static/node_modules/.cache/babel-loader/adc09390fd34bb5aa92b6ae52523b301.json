{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'MainApp',\n  props: {},\n  computed: {\n    lpains() {\n      return this.pains.filter(pain => pain.status == 'sender');\n    }\n\n  },\n\n  mounted() {\n    axios.get('/api/config').then(response => this.pains = response.data);\n  },\n\n  methods: {\n    setIp(event) {\n      const value = event.target.value;\n      const origpai = event.target.data.origpai;\n      console.log(value);\n      console.log(origpai);\n    }\n\n  },\n\n  data() {\n    return {\n      pains: null\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,SADO;AAEbC,OAAK,EAAE,EAFM;AAIbC,UAAQ,EAAE;AACRC,UAAM,GAAE;AACN,aAAO,KAAKC,KAAL,CAAWC,MAAX,CAAkBC,IAAG,IAAKA,IAAI,CAACC,MAAL,IAAe,QAAzC,CAAP;AACF;;AAHQ,GAJG;;AASbC,SAAM,GAAK;AACTT,SAAI,CACDU,GADH,CACO,aADP,EAEGC,IAFH,CAEQC,QAAO,IAAM,KAAKP,KAAL,GAAaO,QAAQ,CAACC,IAF3C;AAGD,GAbY;;AAcbC,SAAO,EAAE;AACPC,SAAK,CAACC,KAAD,EAAQ;AACX,YAAMC,KAAI,GAAID,KAAK,CAACE,MAAN,CAAaD,KAA3B;AACA,YAAME,OAAM,GAAIH,KAAK,CAACE,MAAN,CAAaL,IAAb,CAAkBM,OAAlC;AACAC,aAAO,CAACC,GAAR,CAAYJ,KAAZ;AACAG,aAAO,CAACC,GAAR,CAAYF,OAAZ;AACF;;AANO,GAdI;;AAsBbN,MAAG,GAAK;AACN,WAAO;AACLR,WAAK,EAAE;AADF,KAAP;AAGF;;AA1Ba,CAAf","names":["axios","name","props","computed","lpains","pains","filter","pain","status","mounted","get","then","response","data","methods","setIp","event","value","target","origpai","console","log"],"sourceRoot":"","sources":["/home/dfrancos/devel/personal/pan/pan/static/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div v-for=\"(pai, key) in pains\" v-bind:key=\"pai.ip\">\n  <div :id=pai.ip>\n      <h2>{{key}}</h2>\n      <select v-model=pai.status>\n        <option value=disabled>Disabled</option>\n        <option value=emitter>Sender</option>\n        <option value=receiver>Receiver</option>\n      </select>\n      <div v-if=\"pai.status=='receiver'\"><select v-on:change=setIp><option data-origpai=pai.ip v-for=\"(lpai, lkey) in lpains\" v-bind:key=lpai.ip value=lpai.ip>{{lkey}}</option></select>\n  </div>\n  </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'MainApp',\n  props: {\n  },\n  computed: {\n    lpains(){\n      return this.pains.filter(pain => pain.status == 'sender');\n    }\n  },\n  mounted () {\n    axios\n      .get('/api/config')\n      .then(response => (this.pains = response.data))\n  },\n  methods: {\n    setIp(event) {\n      const value = event.target.value;\n      const origpai = event.target.data.origpai;\n      console.log(value);\n      console.log(origpai);\n    }\n  },\n  data () {\n    return {\n      pains: null\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}